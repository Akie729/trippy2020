<%= form_with model: @trip, local: true do |f| %>
  <%= f.text_field :name, placeholder: "name" %><br>
  <%= f.text_field :title, placeholder: "title" %><br><br>
  <%= f.file_field :images, multiple: true %><br>
  <%= f.text_area :text, placeholder: "text", rows: "10" %><br><br>
  <%= f.submit "SEND" %>
<%end%>