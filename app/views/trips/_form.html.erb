<%= form_with model: @trip, local: true do |f| %>
  <%= f.text_field :name, placeholder: "name" %>
  <div class="field">
    <%= f.label :title, "title" %><br />
    <%= f.text_field :title%>
  </div>
  <div class="field">
    <%= f.label :image,"画像" %><br />
    <%= f.file_field :image %>
  </div>
  <%= f.text_area :text, placeholder: "text", rows: "10" %>
  <%= f.submit "SEND" %>
<%end%>
